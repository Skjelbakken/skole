int my_prime_factor(int number, int prime_factor)
{
    for (int i = 2; i <= prime_factor/2; i++)
    {
        if (i % 2 == 0)
        {
            return 0;
        }
        
    }
    
    if (prime_factor % number == 0){

        return 1;
    }
    else
    {
       return 0;
    }
   
}

// Write a C function that prints numbers in a given range, 
// along with an indication of whether the number is odd or even and whether 5 is a prime factor in the numbers
void prime_factor_range(int start, int end)
{
    int i;
    for ( i = start; i < end; i++)
    {
        if (i % 2 == 0){
            printf("%d is an even number",i);
        }
        else{
            printf("%d is a odd number",i);
        }
        if (my_prime_factor(i, 5)){
            printf(" and %d is a prime factor in %d\n",i, 5);
        }
        else{
            printf(" and %d is not a prime factor in %d\n",i, 5);
        }
        printf("\n");
    }
    
   
}
